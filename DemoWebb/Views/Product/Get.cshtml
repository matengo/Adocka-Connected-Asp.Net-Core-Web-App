@model DemoWebb.Models.Product

@{
    ViewData["Title"] = "Get";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-4">
        <img src ="@Model.ImageUrl" />
    </div>
    <div class="col-md-8">
        <h2>@Model.Name</h2>
        <p>
            @Model.ShortDescription
        </p>
        <p>
            @Html.Raw(Model.Description)
        </p>
    </div>
</div>